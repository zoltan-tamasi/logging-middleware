{
  "name": "logger-middleware",
  "version": "1.0.0",
  "description": "Logging middleware for web APIs",
  "main": "server.js",
  "scripts": {
    "build": "rm -rf build && npm run compile",
    "build:watch": "node_modules/typescript/bin/tsc -w",
    "start": "node build/server.js",
    "dev": "nodemon build/server.js",
    "devWin": "nodemon build/server.js",
    "test": "jest",
    "testWatch": "npm run test -- --watch",
    "compile": "node_modules/typescript/bin/tsc",
    "upgradeDependencies": "npx npm-check-updates --target=minor -u"
  },
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "body-parser": "1.19.1",
    "cls-rtracer": "2.6.0",
    "compression": "1.7.4",
    "cookie-session": "1.4.0",
    "csurf": "1.11.0",
    "dotenv": "8.6.0",
    "express": "4.17.2",
    "express-prom-bundle": "6.4.1",
    "express-winston": "4.2.0",
    "helmet": "4.6.0",
    "mongoose": "6.1.3",
    "node-fetch": "2.6.6",
    "prom-client": "13.2.0",
    "supertest": "6.1.6",
    "typescript": "4.4.4",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/jest": "27.0.3",
    "@types/node": "^22.5.1",
    "jest": "26.6.3",
    "nodemon": "2.0.15",
    "npm-check-updates": "12.0.5"
  }
}
